@page "/"
@using Model;
@using Services
@inject IProfessionalService service

<AboutMe ProfessionalId="ProfessionalId"></AboutMe>

@code{
    public int ProfessionalId { get; set; }
    protected override async  Task OnInitializedAsync()
    {
        Professional professional = await service.GetProfessionalAsync(2);
        ProfessionalId = professional != null ? professional.Id : 1;
        base.OnInitializedAsync();
    }
}